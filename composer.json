{
  "name": "farmatholin/segment-io-bundle",
  "type": "symfony-bundle",
  "description": "Segment.io php lib",
  "keywords": [
    "farmatholin",
    "segmentio",
    "segment",
    "bundle",
    "doctrine",
    "symfony"
  ],
  "version" : "1.6.0",
  "homepage": "http://github.com/farmatholin/SegmentIoBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladislav Marin",
      "email": "farmatholin@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "segmentio/analytics-php": "^3.5",
    "psr/log": "^1.0|^2.0|^3.0",
    "symfony/config": ">= 2.7",
    "symfony/dependency-injection": ">= 2.7",
    "symfony/http-kernel": ">= 2.7",
    "symfony/security-core": ">= 2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4.3"
  },
  "archive": {
    "exclude": ["tests"]
  },
  "autoload": {
    "psr-4": {
      "Farmatholin\\SegmentIoBundle\\": "."
    },
    "exclude-from-classmap": ["/tests/"]
  },
  "suggest": {
    "doctrine/annotations": "This package is required to use annotations instead of PHP attributes"
  }
}
